<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Controller.VocabularyDetailActivity">

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <TextView
                android:text="TITLE"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/vocabularyDetailTitle"/>
        <TextView
                android:text="MEANING"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/vocabularyDetailMeaning"/>
        <TextView
                android:text="EXAMPLE1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/vocabularyDetailExample1"/>
        <TextView
                android:text="EXAMPLE2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/vocabularyDetailExample2"/>
        <TextView
                android:text="EXAMPLE3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/vocabularyDetailExample3"/>
    </LinearLayout>
</android.support.constraint.ConstraintLayout>